@model IEnumerable<BIID.Entities.SeedProduction>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutOfAdmin.cshtml";
}

<h2>List of Seed Production</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Item
        </th>
        <th>
            ProcedureOfSeed
        </th>
        <th>
            Condition
        </th>
        <th>
            SeedStorage
        </th>
        <th>
            Remarks
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
         <td>
            @Html.DisplayFor(modelItem => item.Item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProcedureOfSeed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Condition)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SeedStorage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Remarks)
        </td>
       
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

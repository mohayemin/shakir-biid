    Layout = "~/Views/Shared/_LayoutOfAdmin.cshtml";
}
}

<h2>Details User</h2>

<fieldset>
    <legend>User</legend>

    
     <div class="display-label">AuthorizationLevel</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.RoleId)
    </div>
    
    <div class="display-label">UserName</div>

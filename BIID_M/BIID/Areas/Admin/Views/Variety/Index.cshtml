@model IEnumerable<BIID.Entities.Variety>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutOfAdmin.cshtml";
}
<h2>List of Variety</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
       <th>
            Item
        </th>
       
        <th>
            VarietyName
        </th>
        <th>
            Kharip1
        </th>
        <th>
            Kharip2
        </th>
        <th>
            RobiIndustrious
        </th>
        <th>
            AlltheYear
        </th>
        <th>
            DetailsSupplierInfo
        </th>
        <th>
            Remarks
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Item.Name)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.VarietyName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Kharip1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Kharip2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RobiIndustrious)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AlltheYear)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DetailsSupplierInfo.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Remarks)
        </td>
       
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

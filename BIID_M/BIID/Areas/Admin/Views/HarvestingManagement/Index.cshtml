@model IEnumerable<BIID.Entities.HarvestingManagement>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutOfAdmin.cshtml";
}

<h2>List of Harvesting Management</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Item
        </th>
        <th>
            HarvestingTime
        </th>
        <th>
            Systems
        </th>
        <th>
            Procedures
        </th>
        <th>
            Threshing
        </th>
        <th>
            Packaging
        </th>
        <th>
            Storage
        </th>
        <th>
            Remarks
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
         <td>
            @Html.DisplayFor(modelItem => item.Item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HarvestingTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Systems)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Procedures)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Threshing)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Packaging)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Storage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Remarks)
        </td>
       
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
